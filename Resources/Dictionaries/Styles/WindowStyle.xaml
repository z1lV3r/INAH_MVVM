<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="Window" x:Key="WindowStyle">
        <Setter Property="Template" Value="{StaticResource WindowTemplate}" />
        <Setter Property="Height" Value="664.474" />
        <Setter Property="Title" Value="{Binding Title, Mode=OneTime}" />
        <Setter Property="MinHeight" Value="670" />
        <Setter Property="Width" Value="942.105" />
        <Setter Property="MinWidth" Value="942.105" />
        <Setter Property="Background" Value="White" />
    </Style>
    <Style TargetType="Label" x:Key="WindowTitleStyle">
        <Setter Property="FontSize" Value="24" />
        <Setter Property="FontFamily" Value="Nirmala UI" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
    </Style>
</ResourceDictionary>